    "css.completion.completePropertyWithSemicolon": true,
    "css.lint.unknownAtRules": "ignore",
    "database-client.showUser": true,
    "diffEditor.renderSideBySide": false,
    "diffEditor.wordWrap": "on",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.accessibilitySupport": "off",
    "editor.autoClosingComments": "always",
    "editor.autoClosingDelete": "always",
    "editor.autoClosingOvertype": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.sortImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.fontFamily": "Dank Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.guides.indentation": true,
    "editor.hover.above": false,
    "editor.hover.delay": 5000,
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.largeFileOptimizations": false,
    "editor.linkedEditing": true,
    "editor.minimap.autohide": true,
    "editor.minimap.enabled": false,
    "editor.suggest.insertMode": "replace",
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "emmet.includeLanguages": {
      "javascript": "javascriptreact"
    },
    "emmet.preferences": {
      "bem.elementSeparator": "__",
      "bem.modifierSeparator": "--",
      "bem.shortElementPrefix": "-"
    },
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showSuggestionsAsSnippets": true,
    "errorLens.enabled": true,
    "eslint.options": {
      "extensions": [".js", ".jsx", ".ts", ".tsx"]
      // "configFile": ".eslintrc.js"
    },
    "window.zoomLevel": 0,
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
    "eslint.workingDirectories": ["Projects"],
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 1,
    "files.associations": {
      // "**/src/**/*.js": "javascriptreact",
      "**/src/**/*.jsx": "javascriptreact",
      "**/src/assets/**/*.js": "javascript",
      "**/src/config/**/*.js": "javascript",
      "**/src/constants/**/*.js": "javascript",
      "**/src/context/**/*.js": "javascript",
      "**/src/data/**/*.js": "javascript",
      "**/src/services/**/*.js": "javascript",
      "**/src/utils/**/*.js": "javascript"
    },
    "files.autoSave": "off",
    "git.autofetch": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "html-css-class-completion.enableEmmetSupport": true,
    "html.format.unformattedContentDelimiter": "<!--noformat-->",
    "inlineFold.autoFold": false,
    // "liveSassCompile.settings.formats": [
    //   {
    //     "extensionName": ".css",
    //     "format": "expanded",
    //     "savePath": "/dist"
    //   }
    // ],
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "firefox developer edition",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.fullReload": true,
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
    "markdown.preview.fontFamily": "sans-serif",
    "material-icon-theme.folders.theme": "specific",
    "path-intellisense.autoSlashAfterDirectory": true,
    "prettier.arrowParens": "avoid",
    "prettier.bracketSameLine": true,
    "prettier.bracketSpacing": true,
    "prettier.embeddedLanguageFormatting": "auto",
    "prettier.htmlWhitespaceSensitivity": "css",
    "prettier.insertPragma": false,
    "prettier.jsxSingleQuote": false,
    "prettier.printWidth": 100,
    "prettier.proseWrap": "preserve",
    "prettier.quoteProps": "as-needed",
    "prettier.requirePragma": false,
    "prettier.semi": true,
    "prettier.singleAttributePerLine": false,
    "prettier.singleQuote": false,
    "prettier.tabWidth": 2,
    "prettier.trailingComma": "es5",
    "prettier.useTabs": false,
    "prettier.vueIndentScriptAndStyle": false,
    "projectManager.git.baseFolders": ["~/Projects/"],
    "projectManager.projectsLocation": "~/Projects/",
    "reactSnippets.settings.importReactOnTop": false,
    "search.exclude": {
      "**/*.code-search": true,
      "**/bower_components": true,
      "**/node_modules": true
    },
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.untrustedFiles": "open",
    "settings.cycle": [
      {
        "id": "copilot",
        "overrideWorkspaceSettings": true,
        "values": [
          {
            "github.copilot.editor.enableAutoCompletions": false
          },
          {
            "github.copilot.editor.enableAutoCompletions": true
          }
        ]
      }
    ],
    "sortJSON.orderOverride": ["name", "version", "description", "private", "keywords"],
    "terminal.external.osxExec": "hyper.app",
    "terminal.external.windowsExec": "C:\\WINDOWS\\System32\\wsl.exe -d Ubuntu-20.04",
    "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.env.osx": {
      "FIG_NEW_SESSION": "1",
      "Q_NEW_SESSION": "1"
    },
    "terminal.integrated.fontFamily": "DroidSansMono Nerd Font",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.profiles.windows": {
      "C:\\WINDOWS\\System32\\wsl.exe -d Ubuntu-20.04": {
        "args": [],
        "path": "C:\\WINDOWS\\System32\\wsl.exe -d Ubuntu-20.04"
      }
    },
    "turboConsoleLog.addSemicolonInTheEnd": true,
    "turboConsoleLog.delimiterInsideMessage": "-",
    "turboConsoleLog.includeFileNameAndLineNum": false,
    "turboConsoleLog.insertEnclosingClass": false,
    "turboConsoleLog.insertEnclosingFunction": false,
    "typescript.format.semicolons": "insert",
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.commandCenter": false,
    "window.density.editorTabHeight": "compact",
    "window.title": "${activeEditorMedium}",
    "workbench.colorTheme": "Panda Syntax",
    "workbench.editor.decorations.badges": true,
    "workbench.editor.decorations.colors": true,
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 20,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.wrapTabs": true,
    "workbench.layoutControl.enabled": false,
    "workbench.preferredDarkColorTheme": "Panda Syntax",
    "workbench.preferredLightColorTheme": "Solarized Light",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "bottom",
    "workbench.editor.editorActionsLocation": "titleBar",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "console-ninja.featureSet": "Community",
    "workbench.iconTheme": "material-icon-theme",
    "github.copilot.preferredAccount": "JeremichShane-FS"
